$use {
	stdl.fmt
}

proc main() {
	loop i in 1..10 {
		fmt.print(i _ " ")
	}
	fmt.println()

	loop i = 0 | i < 10 {
		fmt.print(i _ " ")
		i += 1
	}
	fmt.println()

	loop i = 0 | i < 10 | i += 1 {
		fmt.print(i _ " ")
	}
	fmt.println()

	loop i in 0..0 {
		fmt.println(i)
	} else {
		fmt.println("else 0")
	}
}
