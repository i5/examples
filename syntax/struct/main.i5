struct people {
    name
    age
}

struct people2 [name, age]

proc main() {
    print_people(
        people["Lorem Ipsum", 32]
    )
    print_people(
        people{
            "Ipsum Lorem"
            14
        }
    )
}

proc print_people(p) {
    std.println("name: " & p->name)
    std.println("age: " & p->age)
    std.println()
}
