import std

enum {
    red
    green
    blue
}

# enum { red, green, blue }

proc main() {
    print_color(green)
}

proc print_color(color) {
    match color {
        red {
            std.println("red")
        }
        green {
            std.println("green")
        }
        blue {
            std.println("blue")
        }
    }
}
