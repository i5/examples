import {
    "salifm/std" as std
}

proc main() {
    std.print(true and true) # true
    std.print(true and false) # false
    std.print(false and true) # false
    std.print(false and false) # false

    std.print(true or true) # true
    std.print(true or false) # true
    std.print(false or true) # true
    std.print(false or false) # false

    std.print(not true) # false
    std.print(not false and not false) # true
    std.print(not (false or true)) # false
}
