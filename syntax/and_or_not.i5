const std = import("salifm/i5l")

proc main() {
    std.println(true and true) # true
    std.println(true and false) # false
    std.println(false and true) # false
    std.println(false and false) # false

    std.println(true or true) # true
    std.println(true or false) # true
    std.println(false or true) # true
    std.println(false or false) # false

    std.println(not true) # false
    std.println(not false and not false) # true
    std.println(not (false or true)) # false
}
