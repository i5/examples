const std = import("salifm/i5l")
enum colors(red, green, blue)

proc main() {    
    print_color(colors.green)
}

proc print_color(color) {
    when color {
        colors.red {
            std.println("red")
        }
        colors.green {
            std.println("green")
        }
        colors.blue {
            std.println("blue")
        }
    }
    # green
}
